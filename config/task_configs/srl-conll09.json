{
  "srl": {
    "penalty": 1.0,
    "token_take_mask": {
      "layer": "joint_pos_predicate",
      "output": "predicate_predictions"
    },
    "output_fn": {
      "name": "srl_bilinear",
      "params": {
        "predicate_targets": {
          "label": "predicate"
        },
        "predicate_preds_train": {
          "label": "predicate"
        },
        "predicate_preds_eval": {
          "layer": "joint_pos_predicate",
          "output": "predicate_predictions"
        }
      }
    },
    "eval_fns": {
      "srl_f1": {
        "name": "conll09_srl_eval",
        "params": {
          "gold_srl_eval_file": {
            "value": "##save_dir##/srl_gold.txt"
          },
          "pred_srl_eval_file": {
            "value": "##save_dir##/srl_preds.txt"
          },
          "reverse_maps": {
            "reverse_maps": [
              "word",
              "srl"
            ]
          },
          "targets": {
            "layer": "srl",
            "output": "targets"
          },
          "predicate_targets": {
            "label": "predicate"
          },
          "words": {
            "feature": "word"
          },
          "predicate_predictions": {
            "layer": "joint_pos_predicate",
            "output": "predicate_predictions"
          }
        }
      }
    }
  }
}